{
  "name": "seoul",
  "version": "0.2.0",
  "description": "A DataSupplier plugin",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Seoul",
    "manifest": "src/manifest.json",
    "main": "seoul.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Sunki Baek <sunki.baek@gmail.com>",
  "dependencies": {
    "prettier": "^1.14.3"
  }
}
